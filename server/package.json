{
  "name": "express-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "start-dev": "cross-env NODE_ENV=development node ./dist/server.js",
    "nodemon": "cross-env NODE_ENV=development nodemon ./dist/server.js",
    "dev": "concurrently \"yarn:watch\" \"yarn:nodemon\"",
    "test": "concurrently \"yarn:watch\" \"yarn:test-watch\"",
    "test-watch": "jest --watch",
    "prettier": "prettier --write \"**/*.{ts,json,yml}\"",
    "prepare": "husky install"
  },
  "_moduleAliases": {
    "@graphql": "dist/graphql",
    "@interfaces": "dist/interfaces",
    "@lib": "dist/lib",
    "@routes": "dist/routes",
    "@utils": "dist/utils"
  },
  "dependencies": {
    "concurrently": "^7.0.0",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.0.1",
    "helmet": "^4.6.0",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.5.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "lint-staged": "^12.0.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.5.2"
  }
}
